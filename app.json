{
  "expo": {
    "name": "La7agli",
    "slug": "la7agli",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "la7agli",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFC107"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFC107"
      },
      "package": "com.la7agli.delivery",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBxXQ4KffIsymQ1EmhgirbzUEjb37uu5x8"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow La7agli to use your location."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "https://la7agli.com"
      },
      "eas": {
        "projectId": "la7agli-delivery"
      }
    }
  }
}